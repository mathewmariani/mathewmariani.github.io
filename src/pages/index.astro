---
import Layout from '../layouts/Page.astro';
import { ProjectsLink, ResumeLink } from '../consts';
---

<Layout>
	<div class="alert alert-warning" role="alert">
		<h4 class="alert-heading">Whoops!</h4>
		<hr>
		<p class="mb-0">My website is still under development. But the following pages are still available.</p>
		<ul>
			<li><a href={ResumeLink()} class="alert-link">Resume</a></li>
			<li><a href={ProjectsLink()} class="alert-link">Projects</a></li>
		</ul>
	</div>
</Layout>
